package CapaDatos;

public class ConfigurationDummie {

	// atributos
	public int year;
	public int month;
	public String inputLanguage;
	public String outputLanguage;

	// Contructor
	public ConfigurationDummie(int year, int month, String inputLanguage, String outputLanguage) {
		super();
		this.year = year;
		this.month = month;
		this.inputLanguage = inputLanguage;
		this.outputLanguage = outputLanguage;
	}

	// m√©todos

	public static void main(String[] args) {

		ConfigurationDummie configuration = new ConfigurationDummie(2023, 02, "CAT", "ENG");

	}
}
